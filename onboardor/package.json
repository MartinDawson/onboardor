    "relay-compiler-language-typescript": "^0.9.0",
    "ts-loader": "^4.4.2",
    "graphql-cli": "^2.15.8",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-relay": "^1.4.1",
