{
  "name": "onboardor",
  "version": "1.0.0",
  "description": "Helps companies create a good on-boarding experience for their developers.",
  "author": "Martin Dawson <u1356770@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/MartinDawson/onboardor.git"
  },
  "bugs": {
    "url": "https://github.com/MartinDawson/onboardor/issues"
  },
  "keywords": [
    "onboard",
    "developers",
    "onboardor",
    "companies",
    "software",
    "software developers",
    "engineers",
    "onboardor.com"
  ],
  "homepage": "https://github.com/MartinDawson/onboardor",
  "license": "UNLICENSED",
  "scripts": {
    "prestart": "webpack",
    "setup": "run-s restore-nuget",
    "restore-nuget": "dotnet restore",
    "build": "dotnet restore && dotnet build && dotnet publish -o site/onboardor && npm run build-client && copyfiles aws-windows-deployment-manifest.json ./site",
    "start": "cross-env ASPNETCORE_ENVIRONMENT=Development run-p run relay-compiler:watch",
    "run": "dotnet run",
    "relay-compiler": "relay-compiler --src ./Components --schema schema.graphql --language typescript --artifactDirectory ./Components/__generated__ --extensions tsx",
    "relay-compiler:watch": "npm run relay-compiler -- --watch",
    "get-schema": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 graphql get-schema dev",
    "lint": "run-p tslint typescript",
    "tslint": "tslint --fix ./Components/**/*.{ts,tsx}",
    "typescript": "tsc",
    "build-client": "npm run relay-compiler && rimraf ./build && webpack",
    "extension-relay-compiler": "relay-compiler --src ./extension/Components --schema schema.graphql --language typescript --artifactDirectory ./Components/__generated__ --extensions tsx",
    "build-extension-dev": "cross-env ASPNETCORE_ENVIRONMENT=Development NODE_ENV=development && npm run extension-relay-compiler && rimraf ./extension/build ./extension.zip && webpack --config ./extension/webpack.config.js && cd ./extension && zip -r ../extension.zip ./manifest.json ./build/* && cd ..",
    "build-extension": "npm run extension-relay-compiler && rimraf ./extension/build ./extension.zip && webpack --config ./extension/webpack.config.js && cd ./extension && zip -r ../extension.zip ./manifest.json ./build/* && cd .."
  },
  "browserslist": [
    "> 3%",
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "edge >= 12",
    "Explorer 11"
  ],
  "devDependencies": {
    "@types/chrome": "^0.0.71",
    "@types/classnames": "^2.2.6",
    "@types/grid-styled": "^4.1.0",
    "@types/react": "^16.4.4",
    "@types/react-dom": "^16.0.6",
    "@types/react-google-recaptcha": "^0.9.1",
    "@types/react-helmet": "^5.0.6",
